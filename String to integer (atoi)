#include <iostream>
#include <string>

int main(int argv, char* argc[])
{
	std::string s{ "  001" };
	bool ifirst{ false }, num{false};

	for (int i = 0; i < s.size(); i++)
	{
		std::cerr << s << '\t' << s.size() << '\t' << i << std::endl;
		if (ifirst == true)
		{
			--i;
			ifirst = false;
		}
		if (s[i] == 32)
		{
			s.erase(i, 1);
			if (i > 0)
				--i;
			else
				ifirst = true;
			continue;
		}
		else if (num != true)
		{
			if (s[i] == 48)
			{
				s.erase(i, 1);
				if (i > 0)
					--i;
				else
					ifirst = true;
				continue;
			}
		}
		else if ((s[i] == 45) || (s[i] == 43))
		{
			if (i > 0)
			{
				if ((s[i - 1] >= 48) || (s[i - 1] <= 57) && (i > 0))
				{
					s.erase();
					s.resize(1);
					s[0] = 0;
				}
				else
				{
					continue;
				}
			}
		}
		else if ((s[i] < 48) || (s[i] > 57))
		{
			s.erase(i, s.size());
			if (s.empty())
			{
				s.resize(1);
				s[0] = 0;
			}
		}
		else if ((s[i] > 48) || (s[i] <= 57))
		{
			num = true;
		}
	}
	std::cerr << s << '\t' << s.size() <<  std::endl;

	if ((s[0] == '-') && (s.size() >= 11))
		std::cout << std::numeric_limits<int>::min() << '\t' << "min";
	if (s.size() >= 10)
		std::cout << std::numeric_limits<int>::max() << '\t' << "max";

	std::cout << std::atoi(s.c_str()) << std::endl;
	return 0;
}
