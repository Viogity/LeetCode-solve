#include <iostream>
#include <string>

int main(int argv, char* argc[])
{
	std::string s{ "-99999999999999999999999" };
	bool ifirst = false;

	for (int i = 0; i < s.size(); i++)
	{
		std::cerr << s << '\t' << s.size() << '\t' << i << std::endl;
		if (ifirst == true)
		{
			--i;
			ifirst = false;
		}
		std::cerr << s << '\t' << s.size() << '\t' << i << std::endl;
		if (s[i] == 32)
		{
			s.erase(i, 1);
			if (i > 0)
				--i;
			else
				ifirst = true;
			continue;
		}
		else if ((s[i] == 45) || (s[i] == 43))
		{
			if (i > 0)
			{
				if ((s[i - 1] >= 48) || (s[i - 1] <= 57) && (i > 0))
				{
					s.erase();
					s.resize(1);
					s[0] = 0;
				}
				else
				{
					continue;
				}
			}
		}
		else if ((s[i] < 48) || (s[i] > 57))
		{
			s.erase(i,s.size());
			if (s.empty())
			{
				s.resize(1);
				s[0] = 0;
			}
		}
	}
	
	std::cout << std::atoi(s.c_str()) << std::endl;
	return 0;
}
